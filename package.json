{
  "name": "sentrypede",
  "version": "1.0.0",
  "description": "Automated Slack agent that monitors Sentry for errors and creates AI-powered fixes",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
