    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "express": "^5.1.0",
    "node-cron": "^3.0.3",
    "simple-git": "^3.22.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/node-cron": "^3.0.11",
