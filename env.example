# Node.js Environment
NODE_ENV=development
LOG_LEVEL=info
PORT=3000

# Sentry Configuration
SENTRY_AUTH_TOKEN=your_sentry_auth_token_here
SENTRY_ORG_SLUG=your_organization_slug
SENTRY_PROJECT_SLUGS=project1,project2,project3
SENTRY_ENVIRONMENTS=production,staging
SENTRY_POLL_INTERVAL_MS=60000

# Sentry OAuth Configuration (optional - use instead of auth token)
# SENTRY_OAUTH_CLIENT_ID=your_oauth_client_id_here
# SENTRY_OAUTH_CLIENT_SECRET=your_oauth_client_secret_here
# SENTRY_OAUTH_REDIRECT_URI=http://localhost:3000/oauth/callback
# SENTRY_OAUTH_SCOPE=project:read,org:read,event:read

# Slack Configuration
SLACK_BOT_TOKEN=xoxb-your-bot-token-here
SLACK_APP_TOKEN=xapp-your-app-token-here
SLACK_CHANNEL_ID=C1234567890
SLACK_SIGNING_SECRET=your_signing_secret_here

# Google Gemini Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-pro
GEMINI_MAX_TOKENS=4096

# GitHub Configuration
GITHUB_TOKEN=ghp_your_github_token_here
GITHUB_OWNER=your_github_username_or_org
GITHUB_REPO=your_repository_name
GITHUB_DEFAULT_BRANCH=main 